                        <xsd:documentation xml:lang="en">User's Email.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="DeleteAccountRelationResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который приходит на запрос DeleteAccountRelationRequest.
