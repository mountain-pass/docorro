        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="1" minOccurs="0" name="accountRelation" type="tns:AccountRelation">
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="SaveAccountRelationRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Запрос на сохранение делегированного доступа к счету.</xsd:documentation>
            <xsd:documentation xml:lang="en">Request for saving detailed information of delegated access.</xsd:documentation>
