<?xml version="1.0" encoding="UTF-8"?><wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:schema="http://www.moneta.ru/schemas/messages.xsd" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.moneta.ru/services.wsdl" targetNamespace="http://www.moneta.ru/services.wsdl">
  <wsdl:types>
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.moneta.ru/schemas/messages.xsd" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://www.moneta.ru/schemas/messages.xsd">

    <xsd:simpleType name="Version">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Список версий, который используется в MONETA.MerchantAPI.
                Если функциональность зависит от конкретной версии, то в описании запроса это будет указано дополнительно.
                Если версия не указана, то по умолчанию используется VERSION_1.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                The version enumeration which is used in MONETA.MerchantAPI.
                If functionality depends on certain version it will be described for each request type.
                VERSION_1 is considered by default.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="VERSION_1"/>
            <xsd:enumeration value="VERSION_2"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="Entity">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Базовый тип, содержащий атрибут "version".</xsd:documentation>
            <xsd:documentation xml:lang="en">Base type containing "version" attribute.</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute default="VERSION_1" name="version" type="tns:Version" use="optional"/>
    </xsd:complexType>

    <xsd:simpleType name="Money">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, представляющий значение суммы в операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Type representing transaction amounts.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal">
            <xsd:fractionDigits value="2"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Description">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Описание или комментарий, применяющийся в сложных типах.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Description or comments for using with complex types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="Password">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Строка, представляющая различные виды паролей.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                String representing various passwords.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="OperationStatus">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий статусы операций в системе МОНЕТА.РУ.
                Данный тип может иметь только определенные значения, описанные ниже.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction status type in MONETA.RU.
                This type may have enumerated values.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="INPROGRESS">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">В обработке</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction processing is in progress</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="SUCCEED">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Операция обработана</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction processing completed successfully</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="CANCELED">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Операция отменена</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction processing is canceled</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TAKENIN_NOTSENT">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Уведомление по операции не отправлено (или не принято)</xsd:documentation>
                    <xsd:documentation xml:lang="en">External notification is not sent (or not accepted)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="CREATED">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Операция создана</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction is registered</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="TransactionRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий параметры операции в запросах.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction parameters type for requests.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="payer" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер счета плательщика</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payer account number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="payee" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер счета получателя</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payee account number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="amount" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Сумма операции.
                        Если флаг isPayerAmount отсутствует
                        и пользователь имеет доступ только к счету плательщика,
                        то интерпретируется как сумма к списанию (в валюте плательщика).
                        Если флаг isPayerAmount отсутствует
                        и пользователь имеет доступ только к счету получателя,
                        то интерпретируется как сумма к зачислению (в валюте получателя).
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction amount.
                        If isPayerAmount is omitted
                        and user has access only to payer account,
                        then is interpreted as debit amount (in payer's currency).
                        If isPayerAmount is omitted
                        and user has access only to payee account,
                        then is interpreted as credit amount (in payee's currency).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="isPayerAmount" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Если пользователь имеет доступ как к счету плательщика, так и счету получателя,
                        то флаг isPayerAmount обязателен.
                        Если флаг isPayerAmount установлен (true), то amount используется как сумма к списанию (в валюте плательщика).
                        Если флаг isPayerAmount сброшен (false), то amount используется как сумма к зачислению (в валюте получателя).
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        If user has access to both accounts, then isPayerAmount flag is obligatory.
                        If isPayerAmount flag is "true", then amount is used as debit amount (in payer's currency).
                        If isPayerAmount flag is "false", then amount is used as credit amount (in payee's currency).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="paymentPassword" type="tns:Password">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Платежный пароль счета плательщика</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payment password for payer account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="clientTransaction" type="tns:CTID">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Внешний номер операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">External transaction ID</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="description" type="tns:Description">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Описание операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction description or comments</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="operationInfo" type="tns:OperationInfo">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Набор полей, которые необходимо сохранить в качестве атрибутов операции. Значения дат в формате dd.MM.yyyy HH:mm:ss</xsd:documentation>
                    <xsd:documentation xml:lang="en">Key-value pairs that will be saved as a transaction attributes. Use format dd.MM.yyyy HH:mm:ss for date values</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="paymentPasswordChallenge" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Запрос для платежного пароля</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payment password challenge</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="TransactionBatchRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий параметры операции в запросах в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction parameters type for requests in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="transactional" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий выполнять ли все денежные переводы в одной транзакции.
                        Если transactional = true, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут отменены</li>
                         <li>можно проводить только операции со счетами монеты. Нельзя выводить деньги на внешние платежные системы.</li>
			</ul>
                        Если transactional = false, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут сохранены</li>
                         <li>можно проводить любые операции, которые доступны в TransferRequest</li>
                         <li>если выставить флаг exitOnFailure = false, то при возникновении ошибки можно пропустить операцию
                           и продолжить выполнение операций дальше</li>
			</ul>
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Joining all transfers in batch processing into one transaction.
                        True -- all processed transfers will be rolled back on exceptions, no external transfers are allowed.
                        False -- only current transfer rolled back on exceptions, external transfers are allowed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="exitOnFailure" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий прерывать ли выполнение пакета операций, если произошла ошибка.
                        Используется только при transactional = false
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Stops batch processing on exceptions. Used only in conjunction with transactional=false.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:TransactionRequestType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Набор операций, которые необходимо выполнить в одном пакете.
                        Операции выполняются в том порядке, в котором они переданы в запросе.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Set of transfers to be processed in one batch.
                        Processed in order of appearance.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>        
    
    <xsd:complexType name="TransactionResponseType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий атрибуты операции в ответах.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction attributes type for responses.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="status" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Текущий статус операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Current transaction status</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dateTime" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Время последней модификации операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction modification timestamp</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="transaction" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction ID in MONETA.RU</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="clientTransaction" type="tns:CTID">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Внешний номер операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">External transaction ID</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="TransactionBatchResponseType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий атрибуты операции в ответах в пакетном запросе.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction attributes type for responses in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="error" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Поле содержит описание ошибки, если операция не была проведена.
                        В этом случае поле transaction - пустое. Если операция проведена,
                        то поле error пустое, а поле transaction содержит детали операции.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Error description in case of tranfer processing failure.
                        If transfer processed successfully "error" is empty
                        and "transaction" element contains transaction details.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="transaction" type="tns:TransactionResponseType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OperationInfoBatchResponseType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий атрибуты операции в ответах в пакетном запросе.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction attributes type for responses in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="error" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Поле содержит описание ошибки, если операция не была проведена.
                        В этом случае поле transaction - пустое. Если операция проведена,
                        то поле error пустое, а поле transaction содержит детали операции.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Error description in case of tranfer processing failure.
                        If transfer processed successfully "error" is empty
                        and "transaction" element contains transaction details.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="transaction" type="tns:OperationInfo"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AuthoriseTransactionRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип для запроса на регистрацию операции с блокировкой средств на счете плательщика.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction registration and debiting funds from payer's account request type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:TransactionRequestType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="transactionId" type="xsd:long">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">Номер операции. Необязательное поле.</xsd:documentation>
                            <xsd:documentation xml:lang="en">Transaction ID. Optional.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="AuthoriseTransactionBatchRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий параметры операции в запросах в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction parameters type for requests in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="transactional" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий выполнять ли все денежные переводы в одной транзакции.
                        Если transactional = true, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут отменены</li>
                         <li>можно проводить только операции со счетами монеты. Нельзя выводить деньги на внешние платежные системы.</li>
			            </ul>
                        Если transactional = false, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут сохранены</li>
                         <li>можно проводить любые операции, которые доступны в TransferRequest</li>
                         <li>если выставить флаг exitOnFailure = false, то при возникновении ошибки можно пропустить операцию
                           и продолжить выполнение операций дальше</li>
			        </ul>
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Joining all transfers in batch processing into one transaction.
                        True -- all processed transfers will be rolled back on exceptions, no external transfers are allowed.
                        False -- only current transfer rolled back on exceptions, external transfers are allowed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="exitOnFailure" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий прерывать ли выполнение пакета операций, если произошла ошибка.
                        Используется только при transactional = false
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Stops batch processing on exceptions. Used only in conjunction with transactional=false.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:AuthoriseTransactionRequestType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Набор операций, которые необходимо выполнить в одном пакете.
                        Операции выполняются в том порядке, в котором они переданы в запросе.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Set of transfers to be processed in one batch.
                        Processed in order of appearance.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>    

    <xsd:complexType name="ConfirmTransactionRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип для запроса подтверждения операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		        Transaction confirmation request type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="transactionId" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction ID</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="paymentPassword" type="tns:Password">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Платежный пароль счета плательщика</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payment password for payer account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="protectionCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Код протекции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Protection code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="operationInfo" type="tns:OperationInfo">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Набор полей, которые необходимо сохранить в качестве атрибутов операции. Значения дат в формате dd.MM.yyyy HH:mm:ss</xsd:documentation>
                    <xsd:documentation xml:lang="en">Key-value pairs that will be saved as a transaction attributes. Use format dd.MM.yyyy HH:mm:ss for date values</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="amount" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Сумма операции.
                        Если флаг isPayerAmount отсутствует
                        и пользователь имеет доступ только к счету плательщика,
                        то интерпретируется как сумма к списанию (в валюте плательщика).
                        Если флаг isPayerAmount отсутствует
                        и пользователь имеет доступ только к счету получателя,
                        то интерпретируется как сумма к зачислению (в валюте получателя).
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction amount.
                        If isPayerAmount is omitted
                        and user has access only to payer account,
                        then is interpreted as debit amount (in payer's currency).
                        If isPayerAmount is omitted
                        and user has access only to payee account,
                        then is interpreted as credit amount (in payee's currency).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="isPayerAmount" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Если пользователь имеет доступ как к счету плательщика, так и счету получателя,
                        то флаг isPayerAmount обязателен.
                        Если флаг isPayerAmount установлен (true), то amount используется как сумма к списанию (в валюте плательщика).
                        Если флаг isPayerAmount сброшен (false), то amount используется как сумма к зачислению (в валюте получателя).
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        If user has access to both accounts, then isPayerAmount flag is obligatory.
                        If isPayerAmount flag is "true", then amount is used as debit amount (in payer's currency).
                        If isPayerAmount flag is "false", then amount is used as credit amount (in payee's currency).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="paymentPasswordChallenge" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Запрос для платежного пароля</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payment password challenge</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ConfirmTransactionBatchRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип для запроса подтверждения операции в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		        Batch mode transaction confirmation request type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="transactional" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий выполнять ли все денежные переводы в одной транзакции.
                        Если transactional = true, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут отменены</li>
                         <li>можно проводить только операции со счетами монеты. Нельзя выводить деньги на внешние платежные системы.</li>
			</ul>
                        Если transactional = false, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут сохранены</li>
                         <li>можно проводить любые операции, которые доступны в TransferRequest</li>
                         <li>если выставить флаг exitOnFailure = false, то при возникновении ошибки можно пропустить операцию
                           и продолжить выполнение операций дальше</li>
			</ul>
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Joining all transfers in batch processing into one transaction.
                        True -- all processed transfers will be rolled back on exceptions, no external transfers are allowed.
                        False -- only current transfer rolled back on exceptions, external transfers are allowed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="exitOnFailure" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий прерывать ли выполнение пакета операций, если произошла ошибка.
                        Используется только при transactional = false
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Stops batch processing on exceptions. Used only in conjunction with transactional=false.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:ConfirmTransactionRequestType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Набор операций, которые необходимо выполнить в одном пакете.
                        Операции выполняются в том порядке, в котором они переданы в запросе.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Set of transfers to be processed in one batch.
                        Processed in order of appearance.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CancelTransactionRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип для запроса на отмену операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		        Transaction cancelation request type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="transactionId" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction ID</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="protectionCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Код протекции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Protection code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="description" type="tns:Description">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Описание операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction description or comments</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CancelTransactionResponseType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий атрибуты при ответе на запрос отмены операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		        Transaction cancelation response type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="transactionId" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction ID</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="operationStatus" type="tns:OperationStatus">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Статус операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction status</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CancelTransactionBatchRequestType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип для запроса на отмену операций в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		        Batch mode transaction cancelation request type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="transactional" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий выполнять ли все денежные переводы в одной транзакции.
                        Если transactional = true, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут отменены</li>
                         <li>можно проводить только операции со счетами монеты. Нельзя выводить деньги на внешние платежные системы.</li>
			</ul>
                        Если transactional = false, то:<ul>
                         <li>при возникновении ошибки все проведенные операции будут сохранены</li>
                         <li>можно проводить любые операции, которые доступны в TransferRequest</li>
                         <li>если выставить флаг exitOnFailure = false, то при возникновении ошибки можно пропустить операцию
                           и продолжить выполнение операций дальше</li>
			</ul>
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Joining all transfers in batch processing into one transaction.
                        True -- all processed transfers will be rolled back on exceptions, no external transfers are allowed.
                        False -- only current transfer rolled back on exceptions, external transfers are allowed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="exitOnFailure" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Флаг, указывающий прерывать ли выполнение пакета операций, если произошла ошибка.
                        Используется только при transactional = false
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Stops batch processing on exceptions. Used only in conjunction with transactional=false.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:CancelTransactionRequestType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Набор операций, которые необходимо выполнить в одном пакете.
                        Операции выполняются в том порядке, в котором они переданы в запросе.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Set of transfers to be processed in one batch.
                        Processed in order of appearance.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CancelTransactionBatchResponseType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий атрибуты при ответе на запрос отмены операций в пакетной режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		        Transaction cancelation response in batch mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="error" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Поле содержит описание ошибки, если операция не была проведена.
                        В этом случае поле transaction - пустое. Если операция проведена,
                        то поле error пустое, а поле transaction содержит детали операции.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Error description in case of tranfer processing failure.
                        If transfer processed successfully "error" is empty
                        and "transaction" element contains transaction details.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="transaction" type="tns:CancelTransactionResponseType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="InvoiceRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Выставление счета к оплате.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Making new invoice (request for payment).
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" name="payer" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер счета плательщика</xsd:documentation>
                        <xsd:documentation xml:lang="en">Payer account number</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="payee" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер счета получателя</xsd:documentation>
                        <xsd:documentation xml:lang="en">Payee account number</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="amount" type="tns:Money">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Сумма</xsd:documentation>
                        <xsd:documentation xml:lang="en">Amount</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="clientTransaction" type="tns:CTID">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Внешний номер операции</xsd:documentation>
                        <xsd:documentation xml:lang="en">external transaction identificator</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="description" type="tns:Description">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Описание операции</xsd:documentation>
                        <xsd:documentation xml:lang="en">Transaction description or comments</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="mnt_custom1" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Произвольный параметр</xsd:documentation>
                        <xsd:documentation xml:lang="en">Custom parameter</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="mnt_custom2" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Произвольный параметр</xsd:documentation>
                        <xsd:documentation xml:lang="en">Custom parameter</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="mnt_custom3" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Произвольный параметр</xsd:documentation>
                        <xsd:documentation xml:lang="en">Custom parameter</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="operationInfo" type="tns:OperationInfo">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Набор полей, которые необходимо сохранить в качестве атрибутов операции. Значения дат в формате dd.MM.yyyy HH:mm:ss</xsd:documentation>
                        <xsd:documentation xml:lang="en">Key-value pairs that will be saved as a transaction attributes. Use format dd.MM.yyyy HH:mm:ss for date values</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="InvoiceResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на выставление счета к оплате.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Invoice registration response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:TransactionResponseType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="TransferRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на перевод денежных средств.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Money Transfer Request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:TransactionRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="TransferResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос перевода денежных средств.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Money transfer registration response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:TransactionResponseType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="TransferBatchRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на перевод денежных средств в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Money Transfer Request in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:TransactionBatchRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="TransferBatchResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос перевода денежных средств в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Money transfer registration response in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:TransactionBatchResponseType">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Детали проведенных операций, либо описание ошибок, если операция не проведена.
                            Порядок соответствует набору операций, переданных в TransferBatchRequest.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Either transaction details or error description in order of appearance in corresponding request.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="AuthoriseTransactionRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на регистрацию операции с блокировкой средств на счете плательщика.
                Завершить (подтвердить) операцию можно с помощью ConfirmTransactionRequest.
                Если операция не будет закончена с помощью ConfirmTransactionRequest, то
                операция будет отменена автоматически по истечению срока блокировки.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Request for transaction registration and debiting funds from payer's account.
                To finish (confirm) this transaction use ConfirmTransactionRequest.
                Authorised but not cofirmed transactions are cancelled automaticaly after authorisation expiry.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:AuthoriseTransactionRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="AuthoriseTransactionResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос регистрации операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction authorisation response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:OperationInfo">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            В список атрибутов операции добавятся два значения:
                            <ul>
                            <li>protectioncode - код протекции, который необходимо использовать в ConfirmTransactionRequest.</li>
                            <li>protectioncodeexpirationdate - дата, до которой работает код протекции, по истечении срока операция отменяется.</li>
                            </ul>
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Transaction attributes contain:
                            <ul>
                            <li>protectioncode - code which has to be passed to ConfirmTransactionRequest.</li>
                            <li>protectioncodeexpirationdate - authorisation expiry, transaction will be cancelled.</li>
                            </ul>
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:extension>                    
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="AuthoriseTransactionBatchRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на регистрацию операции с блокировкой средств на счете плательщика в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Request for transactions registration and debiting funds from payer's account in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:AuthoriseTransactionBatchRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="AuthoriseTransactionBatchResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос регистрации операций в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transactions authorisation response in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:OperationInfoBatchResponseType">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Детали проведенных операций, либо описание ошибок, если операция не проведена.
                            Порядок соответствует набору операций, переданных в AuthoriseTransactionBatchRequest.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Either transaction details or error description in order of appearance in corresponding request.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ConfirmTransactionRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на завершение (подтверждение) операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction confirmation request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:ConfirmTransactionRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ConfirmTransactionResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос завершения операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction confirmation response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:OperationInfo">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Описание операции.
                            Операция полностью проведена, если statusid="SUCCEED"
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Transaction information.
                            Transaction is completed if statusid="SUCCEED"
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:extension>                    
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ConfirmTransactionBatchRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на завершение (подтверждение) операций в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transactions confirmation request in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:ConfirmTransactionBatchRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ConfirmTransactionBatchResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос завершения операций в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transactions confirmation response in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:OperationInfoBatchResponseType">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Детали проведенных операций, либо описание ошибок, если операция не проведена.
                            Порядок соответствует набору операций, переданных в ConfirmTransactionBatchRequest.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Either transaction details or error description in order of appearance in corresponding request.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="CancelTransactionRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на отмену операции. Отменять операцию можно при выполнении всех условий:
		<ul>
                <li>незавершенные операции;</li>
                <li>если есть доступ к счету плательщика и счету получателя (то есть свои внутренние операции).</li>
		</ul>
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction cancelation request if transasction fulfils following conditions:
		<ul>
                <li>transaction is in processing (incompleted);</li>
                <li>principal has an access to both (payer and payee) accounts.</li>
		</ul>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:CancelTransactionRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="CancelTransactionResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос CancelTransactionRequest.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		Transaction cancelation response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:CancelTransactionResponseType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="CancelTransactionBatchRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на отмену операций в пакетном режиме.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transactions cancelation request in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:CancelTransactionBatchRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="CancelTransactionBatchResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос CancelTransactionBatchRequest.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transactions cancelation response in batch processing mode.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="transaction" type="tns:CancelTransactionBatchResponseType">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Детали проведенных операций, либо описание ошибок, если операция не проведена.
                            Порядок соответствует набору операций, переданных в CancelTransactionBatchRequest.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Either transaction details or error description in order of appearance in corresponding request.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="RefundRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Возврат средств по указанной операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Refund given transaction.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="transactionId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер операции в системе МОНЕТА.РУ, по которой необходимо вернуть деньги</xsd:documentation>
                        <xsd:documentation xml:lang="en">Transaction ID in Moneta.ru</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="amount" type="tns:Money">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Сумма, которую необходимо возвратить.
                            Если сумма не указана, то сумма для возврата берется из указанной операции.
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Amount to refund in source transaction payee's currency.
                            If omitted the source transaction payees' amount is used.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="paymentPassword" type="tns:Password">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Платежный пароль</xsd:documentation>
                        <xsd:documentation xml:lang="en">Payment password for payer account</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="clientTransaction" type="tns:CTID">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Внешний номер операции</xsd:documentation>
                        <xsd:documentation xml:lang="en">External transaction ID</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="description" type="tns:Description">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Описание операции</xsd:documentation>
                        <xsd:documentation xml:lang="en">Transaction description or comments</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="operationInfo" type="tns:OperationInfo">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Набор полей, которые необходимо сохранить в качестве атрибутов операции. Значения дат в формате dd.MM.yyyy HH:mm:ss</xsd:documentation>
                        <xsd:documentation xml:lang="en">Key-value pairs that will be saved as a transaction attributes. Use format dd.MM.yyyy HH:mm:ss for date values</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="paymentPasswordChallenge" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Запрос для платежного пароля</xsd:documentation>
                        <xsd:documentation xml:lang="en">Payment password challenge</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="RefundResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ по запросу возврата средств, содержащий информацию по возвратной операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Response containing refund transaction information.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:OperationInfo">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Описание операции.
                            Операция полностью проведена, если statusid="SUCCEED"
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Transaction information.
                            Transaction is completed if statusid="SUCCEED"
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="ForecastTransactionResponseType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий суммы и комиссии в предварительном расчете операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Type describes amount and fees for preliminary calculation of transaction.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="payer" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер счета плательщика</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payer account number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="payerCurrency" type="tns:Currency">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Валюта счета плательщика</xsd:documentation>
                    <xsd:documentation xml:lang="en">Currency of payer account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="payerAmount" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Сумма к списанию</xsd:documentation>
                    <xsd:documentation xml:lang="en">Debit amount</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="payerFee" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Комиссия списания средств</xsd:documentation>
                    <xsd:documentation xml:lang="en">Debit fee</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="payee" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер счета получателя</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payee account number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="payeeCurrency" type="tns:Currency">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Валюта счета получателя</xsd:documentation>
                    <xsd:documentation xml:lang="en">Currency of payee account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="payeeAmount" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Сумма к зачислению</xsd:documentation>
                    <xsd:documentation xml:lang="en">Credit amount</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="payeeFee" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Комиссия зачисления средств</xsd:documentation>
                    <xsd:documentation xml:lang="en">Credit fee</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="payerAlias" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Название счета плательщика</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payer account alias</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="payeeAlias" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Название счета получателя</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payee account alias</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="VerifyTransferResponseType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий состояние операции в системе МОНЕТА.РУ
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Type describes verification status of transaction.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="isTransactionValid" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Если поле равно true, то данная операция может
                        быть проведена в системе МОНЕТА.РУ
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        If true the transaction may be processed in MONETA.RU
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Описание текущего статуса операции.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Description of current transaction status.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="forecast" type="tns:ForecastTransactionResponseType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Если операция может быть проведена, то в данном поле содержатся детали операции.
                        Иначе - это поле пустое.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Additional information if transaction may be processed. Otherwise empty.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="ForecastTransactionRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
		        Запрос предварительного расчета сумм и комиссий по параметрам операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
		        Request for amount and fee preliminary calculation by transaction parameters.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:TransactionRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ForecastTransactionResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос предварительного расчета сумм и комиссий.
                Может использоваться для информирования пользователя о приблизительной сумме и комиссии
                операции.
                Фактические значения могут отличаться, если к моменту проведения операции
                доступны новые правила тарификации или новые курсы обмена валют.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Response for preliminary calculation request.
                May be used for informing users with approximate amount and fee of transaction.
                Actual values may differ if newer financial rules and currency exchange rates
                are available at the moment of transaction processing.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:ForecastTransactionResponseType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="VerifyTransactionRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос возможности провести операцию в системе МОНЕТА.РУ.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction verification request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="transactionId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Идентификатор операции в системе МОНЕТА.РУ, которую надо проверить.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            The identifier of transaction which has to be verified.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="payer" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер счета плательщика</xsd:documentation>
                        <xsd:documentation xml:lang="en">Payer account number</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="amount" type="tns:Money">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Сумма операции.
                            Если флаг isPayerAmount отсутствует
                            и пользователь имеет доступ только к счету плательщика,
                            то интерпретируется как сумма к списанию (в валюте плательщика).
                            Если флаг isPayerAmount отсутствует
                            и пользователь имеет доступ только к счету получателя,
                            то интерпретируется как сумма к зачислению (в валюте получателя).
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Transaction amount.
                            If isPayerAmount is omitted
                            and user has access only to payer account,
                            then is interpreted as debit amount (in payer's currency).
                            If isPayerAmount is omitted
                            and user has access only to payee account,
                            then is interpreted as credit amount (in payee's currency).
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="isPayerAmount" type="xsd:boolean">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Если пользователь имеет доступ как к счету плательщика, так и счету получателя,
                            то флаг isPayerAmount обязателен.
                            Если флаг isPayerAmount установлен (true), то amount используется как сумма к списанию (в валюте плательщика).
                            Если флаг isPayerAmount сброшен (false), то amount используется как сумма к зачислению (в валюте получателя).
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            If user has access to both accounts, then isPayerAmount flag is obligatory.
                            If isPayerAmount flag is "true", then amount is used as debit amount (in payer's currency).
                            If isPayerAmount flag is "false", then amount is used as credit amount (in payee's currency).
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="operationInfo" type="tns:OperationInfo">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Набор полей, которые необходимо использовать в качестве атрибутов операции. Значения дат в формате dd.MM.yyyy HH:mm:ss</xsd:documentation>
                        <xsd:documentation xml:lang="en">Key-value pairs that will be used as a transaction attributes. Use format dd.MM.yyyy HH:mm:ss for date values</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="VerifyTransactionResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос проверки проведения операции в системе МОНЕТА.РУ
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction verification response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:VerifyTransferResponseType">
                    <xsd:sequence>
                        <xsd:element name="transactionId" type="xsd:long">
                            <xsd:annotation>
                                <xsd:documentation xml:lang="ru">
                                    Идентификатор операции в системе МОНЕТА.РУ.
                                </xsd:documentation>
                                <xsd:documentation xml:lang="en">
                                    The identifier of transaction.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="operationStatus" type="tns:OperationStatus">
                            <xsd:annotation>
                                <xsd:documentation xml:lang="ru">
                                    Статус операции в системе МОНЕТА.РУ.
                                </xsd:documentation>
                                <xsd:documentation xml:lang="en">
                                    Transaction status type in MONETA.RU
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="VerifyTransferRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос возможности провести операцию в системе МОНЕТА.РУ.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction verification request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:TransactionRequestType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="VerifyTransferResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос проверки проведения операции в системе МОНЕТА.РУ
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction verification response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:VerifyTransferResponseType"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:simpleType name="AccountId">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий номер счета в системе МОНЕТА.РУ.
                Счет задается числом, минимальное значение которого равно 1,
                а максимальное состоит из 10 цифр.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account type in MONETA.RU.
                Must contain up to 10 digits, minimum value is 1.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:long">
            <xsd:minInclusive value="1"/>
            <xsd:totalDigits value="10"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="Currency">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий валюты, используемые в системе МОНЕТА.РУ.
                Данный тип может иметь только строго определенные значения, описанные ниже.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Currency type in MONETA.RU.
                Must be one of values listed below.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="RUB"/>
            <xsd:enumeration value="USD"/>
            <xsd:enumeration value="EUR"/>
            <xsd:enumeration value="GBP"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="AccountStatus">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий статусы счетов в системе МОНЕТА.РУ.
                Счет может быть либо активным, либо заблокированным.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account status in MONETA.RU.
                May be either active or inactive.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Активный счет</xsd:documentation>
                    <xsd:documentation xml:lang="en">Active account</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Заблокированный счет</xsd:documentation>
                    <xsd:documentation xml:lang="en">Inactive (blocked) account</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="AccountType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Типы счетов в системе МОНЕТА.РУ.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account types in MONETA.RU.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Обычный счет</xsd:documentation>
                    <xsd:documentation xml:lang="en">Standard account</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Расширенный счет</xsd:documentation>
                    <xsd:documentation xml:lang="en">Advanced account</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Бонус-счет</xsd:documentation>
                    <xsd:documentation xml:lang="en">Bonus account</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="AccountInfo">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий атрибуты счета в системе МОНЕТА.РУ.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account information container type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id" type="tns:AccountId">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер счета в системе МОНЕТА.РУ</xsd:documentation>
                    <xsd:documentation xml:lang="en">Account number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="currency" type="tns:Currency">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Валюта данного счета</xsd:documentation>
                    <xsd:documentation xml:lang="en">Currency of account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="balance" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Баланс на данном счете</xsd:documentation>
                    <xsd:documentation xml:lang="en">Current balance of account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="availableBalance" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Доступный баланс на данном счете</xsd:documentation>
                    <xsd:documentation xml:lang="en">Available balance of account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="type" type="tns:AccountType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Тип счета в системе МОНЕТА.РУ</xsd:documentation>
                    <xsd:documentation xml:lang="en">Type of account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="status" type="tns:AccountStatus">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Статус счета в системе МОНЕТА.РУ</xsd:documentation>
                    <xsd:documentation xml:lang="en">Status of account</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="alias" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Название счета в системе МОНЕТА.РУ. Необязательное поле</xsd:documentation>
                    <xsd:documentation xml:lang="en">Alias of account. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="onSuccessfulDebitUrl" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">URL после списания средств. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">URL on debiting. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="onSuccessfulCreditUrl" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">URL после зачисления средств. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">URL on crediting. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="signature" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Код проверки целостности данных. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">Payment form data integrity signature. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="lowBalanceThreshold" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Если баланс счета меньше данного значения, то раз в сутки уходит уведомление об этом событии. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">Daily notifications if balance is less than threshold. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="highBalanceThreshold" type="tns:Money">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Если баланс счета больше данного значения, то раз в сутки уходит уведомление об этом событии. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">Daily notifications if balance is greater than threshold. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="accountAccess" type="tns:AccountAccessInfo">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Информация о доступе к счету. Информация отдается, если счет является делегированным. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">Account access information. Available if the account is delegated, otherwise omitted. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="prototypeAccountId" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Счет-прототип с которого берутся свойства "по умолчанию". Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">Prototype account for default properties values. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="onCancelledDebitUrl" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">URL после отмены списания средств. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">URL on cancelled debit. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="onCancelledCreditUrl" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">URL после отмены зачисления средств. Необязательное поле.</xsd:documentation>
                    <xsd:documentation xml:lang="en">URL on cancelled credit. Optional.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AccountAccessInfo">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий доступ к счету.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account access description type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="accessToWrite" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Доступ на изменение свойств счета</xsd:documentation>
                    <xsd:documentation xml:lang="en">Account attributes modification access</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="accessToTakenOut" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Доступ на снятие средств со счета</xsd:documentation>
                    <xsd:documentation xml:lang="en">Account debiting access</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="accessToTakenIn" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Доступ на зачисление средств на счет</xsd:documentation>
                    <xsd:documentation xml:lang="en">Account crediting access</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="FindAccountByIdRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на поиск счета по его номеру.
                Счет должен принадлежать текущему пользователю.
                Если счет не найден, то возникает Exception.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account searching by account number request.
                Authenticated user must be authorized for the account.
                Exception is thrown if account is not found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
            <xsd:restriction base="tns:AccountId"/>
        </xsd:simpleType>
    </xsd:element>

    <xsd:element name="FindAccountByIdResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который приходит на запрос FindAccountByIdRequest.
                В ответе содержится информация по счету.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account searching by account number response.
                Response contains Account information.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="account" type="tns:AccountInfo"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindAccountByAliasRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на поиск счета по его названию.
                Счет должен принадлежать текущему пользователю.
                Название в запросе не может быть пустым.
                Если счет не найден, то возникает Exception.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account searching by alias request.
                Authenticated user must be authorized for the account.
                Alias must be not empty.
                Exception is thrown if account is not found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
            <xsd:restriction base="xsd:string"/>
        </xsd:simpleType>
    </xsd:element>

    <xsd:element name="FindAccountByAliasResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который приходит на запрос FindAccountByAliasRequest.
                В ответе содержится информация по счету.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account searching by account alias response.
                Response contains Account information.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="account" type="tns:AccountInfo"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    
    <xsd:element name="FindAccountsListRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение списка счетов по заданным условиям поиска.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Accounts searching by given filter.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" name="unitId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Пользователь, которому принадлежат счета.
                            Если это поле не задано, то используется текущий пользователь.
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Structure element, where accounts belong to.
                            If omitted authenticated user's structure is used.                            
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="alias" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Название счета. Поиск происходит по прямому совпадению.
                            Для задания маски можно указать спец-символы "*" или "?".
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Alias of account. Wildcards "*" and "?" may be used.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>                    
                <xsd:element minOccurs="0" name="currency" type="tns:Currency">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Валюта счета. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Currency of account. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="isDelegatedAccount" type="xsd:boolean">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Является ли счет делегированным. Необязательное поле.
                             - поле не указано - выбрать все счета
                             - false - выбрать только неделегированные счета
                             - true - выбрать только делегированные счета
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">The boolean flag indicating if account is deligated. Optional.
                             - select all accounts if omitted
                             - false - select only non-delegated (owned) accounts
                             - true - select only delegated accounts
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindAccountsListResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который содержит список счетов.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Accounts searching response containing the list of accounts.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="account" type="tns:AccountInfo"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="GetAccountPaymentPasswordChallengeRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Получить запрос для платежного пароля</xsd:documentation>
            <xsd:documentation xml:lang="en">Receive challenge for payment password</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="1" name="accountId" type="tns:AccountId">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер счета</xsd:documentation>
                        <xsd:documentation xml:lang="en">Account number</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="GetAccountPaymentPasswordChallengeResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Ответ содержит строку запроса для платежного пароля</xsd:documentation>
            <xsd:documentation xml:lang="en">Response contains of challenge string for payment password</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" name="paymentPasswordChallenge" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Запрос для платежного пароля</xsd:documentation>
                        <xsd:documentation xml:lang="en">Challenge of payment password</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="CreateAccountRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на создание счета.
                Счет создается для указанного пользователя или,
                если это поле не указано, - для текущего пользователя.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account creation request.
                Account is created for given structure element (unitId)
                or for authenticated user's structure if unitId is omitted.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="currency" type="tns:Currency">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Валюта счета</xsd:documentation>
                        <xsd:documentation xml:lang="en">Currency of account</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="alias" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Название счета.
                            Название счета должно быть уникальным среди счетов одного пользователя.
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Alias of account.
                            Must be unique among accounts of given user.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="1" name="paymentPassword">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Платежный пароль. Минимальная длина - 5 символов
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Payment password. Minimum length is 5 symbols.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:minLength value="5"/>
                            <xsd:whiteSpace value="collapse"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element minOccurs="0" name="unitId" nillable="true" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Пользователь, которому будет принадлежать данный счет.
                            Если это поле не задано, то счет создается для текущего пользователя.
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Structure element, where the account belongs to.
                            If omitted authenticated user's structure is used.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="onSuccessfulDebitUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после списания средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on debiting. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="onSuccessfulCreditUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после зачисления средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on crediting. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="signature" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Код проверки целостности данных. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Mandatory payment form signature. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="lowBalanceThreshold" type="tns:Money">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Если баланс счета меньше данного значения, то раз в сутки уходит уведомление об этом событии. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Daily notifications if balance is less than threshold. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="highBalanceThreshold" type="tns:Money">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Если баланс счета больше данного значения, то раз в сутки уходит уведомление об этом событии. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Daily notifications if balance is greater than threshold. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="prototypeAccountId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Счет-прототип с которого берутся свойства "по умолчанию". Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Prototype account for default properties values. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="onCancelledDebitUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после отмены списания средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on cancelled debit. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="onCancelledCreditUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после отмены зачисления средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on cancelled credit. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="CreateAccountResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который приходит на запрос CreateAccountRequest.
                В ответе содержится номер (AccountId) созданного счета.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account creation response.
                Contains created account identificator.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
            <xsd:restriction base="tns:AccountId"/>
        </xsd:simpleType>
    </xsd:element>

    <xsd:element name="EditAccountRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на редактирование счета.
                У счета можно изменить свойства "псевдоним" и "платежный пароль".
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account modification request.
                It's allowed to change alias and payment password for account.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="id" type="tns:AccountId">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Идентификатор редактируемого счета.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Modifying account's number.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="alias" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Новый псевдоним счета. 
                            Псевдоним счета должен быть уникальным среди счетов пользователя.
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">New alias of account.
                            Must be unique among accounts of given user.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="paymentPassword">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Новый платежный пароль. Для того чтобы изменить платежный
                            пароль, необходимо указать предыдущий платежный пароль. Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">New payment password for account.
                            oldPaymentPassword is required to set new payment password.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:minLength value="5"/>
                            <xsd:whiteSpace value="collapse"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element minOccurs="0" name="oldPaymentPassword">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Текущий платежный пароль. Для того чтобы изменить платежный
                            пароль на новый, необходимо указать текущий платежный пароль. Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Current payment password.
                            Used for authorization of changing payment password to new value.
                            Required only if newPaymentPassword is specified.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:minLength value="5"/>
                            <xsd:whiteSpace value="collapse"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element minOccurs="0" name="onSuccessfulDebitUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после списания средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on debiting. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="onSuccessfulCreditUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после зачисления средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on crediting. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="signature" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Код проверки целостности данных. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Mandatory payment form signature. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="lowBalanceThreshold" type="tns:Money">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Если баланс счета меньше данного значения, то раз в сутки уходит уведомление об этом событии. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Daily notifications if balance is less than threshold. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="highBalanceThreshold" type="tns:Money">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Если баланс счета больше данного значения, то раз в сутки уходит уведомление об этом событии. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Daily notifications if balance is greater than threshold. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="prototypeAccountId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Счет-прототип с которого берутся свойства "по умолчанию". Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Prototype account for default properties values. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="oldPaymentPasswordChallenge" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Запрос для платежного пароля</xsd:documentation>
                        <xsd:documentation xml:lang="en">Payment password challenge</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="onCancelledDebitUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после отмены списания средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on cancelled debit. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element minOccurs="0" name="onCancelledCreditUrl" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">URL после отмены зачисления средств. Необязательное поле.</xsd:documentation>
                        <xsd:documentation xml:lang="en">URL on cancelled credit. Optional.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="EditAccountResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который приходит на запрос EditAccountRequest.
                В ответе нет никаких данных.
                Если в процессе сохранения счета произошла ошибка, то возникнет Exception.
                Если Exception не возник - значит счет гарантированно сохранен.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Account modification response.
                Response contains no data unless some error has occured while modification.
                If reponse has no Exception then account is modified successfully.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType/>
    </xsd:element>

    <xsd:complexType name="KeyValueAttribute">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, который позволяет работать с сущностями типа "ключ-значение".
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Key-value pairs type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="key" type="xsd:string"/>
            <xsd:element name="value" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="KeyValueApprovedAttribute">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, расширяющий KeyValueAttribute.
                Поле approved показывает проверено или нет данное свойство. Поле approved - не обязательное.
                Поле published показывает доступно или нет чтение свойства для всех пользователей. Поле published - не обязательное.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Extended KeyValueAttribute Type.
                Approved element shows if attribute value is approved or not. Approved element is optional.
                Published element shows if attribute value is available for reading for all users. Published element is optional.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:KeyValueAttribute">
                <xsd:sequence>
                    <xsd:element maxOccurs="1" minOccurs="0" name="approved" type="xsd:boolean"/>
                    <xsd:element maxOccurs="1" minOccurs="0" name="published" type="xsd:boolean">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="ru">
                                Для чтения этого атрибута в запросе необходимо выставлять атрибут version равный или больше VERSION_2.
                                Для записи version можно не указывать.
                            </xsd:documentation>
                            <xsd:documentation xml:lang="en">
                                The value "VERSION_2" of request's attribute "version" is required for reading of this attribute.
                                The request's attribute "version" for writing is optional.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="OperationInfo">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, описывающий операцию в системе МОНЕТА.РУ.
                Он представляет собой номер (id) операции и набор полей,
                которые присутствуют у данной операции. Поля представлены в виде "ключ-значение".
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction type contains transaction id and key-value pairs representing transaction attributes.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="id" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер операции</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction ID</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="attribute" type="tns:KeyValueAttribute">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Поля операции. Полей в операции может быть 0 и более.
                        Список полей:
                        <ul>
                        <li>clienttransaction - внешний (не в системе МОНЕТА.РУ) номер операции;</li>
                        <li>statusid - статус операции;</li>
                        <li>typeid - тип операции:
                            <ul>
                                <li>2,10 - оплата с пользовательского счета,</li>
                                <li>3 - оплата с внешней платежной системы,</li>
                                <li>7,14 - пополнение счета,</li>
                                <li>4,13 - вывод на внешнюю платежную систему,</li>
                                <li>11 - вывод на пользовательский счет,</li>
                                <li>12 - внутренний перевод;</li>
                            </ul>
                        </li>
                        <li>category - категория операции:
                            <ul>
                                <li>DEPOSIT - ввод средств,</li>
                                <li>WITHDRAWAL - вывод средств,</li>
                                <li>TRANSFER - внутренний перевод,</li>
                                <li>BUSINESS - товары и услуги;</li>
                            </ul>
                        </li>
                        <li>modified - время последнего изменения операции;</li>
                        <li>sourceaccountid - номер счета, с которого произведена операция;</li>
                        <li>sourcecurrencycode - валюта счета;</li>
                        <li>sourceamount - сумма по операции;</li>
                        <li>sourceamountfee - сумма комиссии;</li>
                        <li>sourceamounttotal - общая сумма с учетом комиссии;</li>
                        <li>targetaccountid - корреспондентский счет;</li>
                        <li>targetalias - название корреспондентского счета;</li>
                        <li>customfield:* - произвольный набор значений.
                            В операции их может быть несколько.
                            Полный ключ атрибута состоит из префикса ("customfield:") и тэга (32 символа). 
                            Например, "customfield:name".</li>
                        </ul>
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Transaction attributes. Attributes set may be empty.
                        Attributes list:
                        <ul>
                        <li>clienttransaction - external transaction ID;</li>
                        <li>statusid - transaction status;</li>
                        <li>typeid - transaction type:
                            <ul>
                                <li>2,10 - payment from user account,</li>
                                <li>3 - payment from external system,</li>
                                <li>7,14 - deposit,</li>
                                <li>4,13 - withdrawal to external system,</li>
                                <li>11 - withdrawal to user account,</li>
                                <li>12 - inner transfer;</li>
                            </ul>
                        </li>
                        <li>category - transaction category:
                            <ul>
                                <li>DEPOSIT,</li>
                                <li>WITHDRAWAL,</li>
                                <li>TRANSFER,</li>
                                <li>BUSINESS;</li>
                            </ul>
                        </li>
                        <li>modified - transaction modification timestamp;</li>
                        <li>sourceaccountid - account number;</li>
                        <li>sourcecurrencycode - account currency;</li>
                        <li>sourceamount - transaction amount;</li>
                        <li>sourceamountfee - transaction commission;</li>
                        <li>sourceamounttotal - total transaction amount incl. commission;</li>
                        <li>targetaccountid - correspondent account number;</li>
                        <li>targetalias - alias of correspondent account;</li>
                        <li>customfield:* - custom list of values.
                            The transaction may contain several attributes with different keys.
                            The full attribute's key consists of prefix ("customfield:") and tag (32 characters).
                            For example, "customfield:name".</li>    
                        </ul>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="CTID">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, представляющий внешний номер операции в учетной системе партнера.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                External transaction identificator type.
                For example, partner's system billing identificator.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="OperationInfoList">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, представляющий список операций. 
                Содержит разбиение по страницам для отображения длинных списков.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction list type.
                Contains paged structure for long list presentation.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="pageSize" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Количество операций, возвращаемых в результате запроса.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction per page within one request.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="pageNumber" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер текущей страницы. Нумерация начинается с 1
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Current page number starting with 1.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="pagesCount" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Максимальное количество страниц с операциями по данному запросу
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Total page count for given request.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="size" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Количество операций на текущей странице.
                        Меньше или равно pageSize.
                        Последняя страница может содержать операций меньше, чем pageSize.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Transactions count in current page.
                        Less or equal to pageSize.
                        Last page may contain less than pageSize transactions.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="totalSize" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Общее количество операций, которое можно получить в данной
                        выборке
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Total transactions count for given request.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="operation" type="tns:OperationInfo">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Список операций</xsd:documentation>
                    <xsd:documentation xml:lang="en">Transaction list</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Pager">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип, позволяющий задать необходимую страницу при отображении длинных списков.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Long list retrieval type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element default="1" name="pageNumber">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер страницы, которую нужно получить. Минимальное значение равно 1.
                        Значение по умолчанию равно 1.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Page number to retrieve. Minimal value is 1.
                        By default is 1.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="1"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element default="10" name="pageSize">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Сколько записей необходимо получить на одной странице. Минимальное
                        значение равно 1. Максимальное значение равно 100. Значение по умолчанию равно 10.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">Transactions per page. Minimal value is 1.
                        Maximal value is 100.
                        By default is 10.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="1"/>
                        <xsd:maxInclusive value="100"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="GetOperationDetailsByIdRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение данных по номеру операции (номер в системе МОНЕТА.РУ).
                Если данные не найдены, то возникает Exception.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction information request by transaction number in MONETA.RU.
                Exception is thrown if no data found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
            <xsd:annotation>
                <xsd:documentation xml:lang="ru">Номер операции в системе МОНЕТА.РУ</xsd:documentation>
                <xsd:documentation xml:lang="en">Transaction ID in MONETA.RU</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:long"/>
        </xsd:simpleType>
    </xsd:element>

    <xsd:element name="GetOperationDetailsByIdResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос получения информации по операции.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction information response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:annotation>
                <xsd:documentation xml:lang="ru">Данные по операции</xsd:documentation>
                <xsd:documentation xml:lang="en">Transaction details</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
                <xsd:element name="operation" type="tns:OperationInfo"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindOperationsListByCTIDRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение данных по внешнему номеру операции
                (номеру не в системе МОНЕТА.РУ).
                По внешнему номеру операции может быть найдено несколько операций
                в системе МОНЕТА.РУ. Поэтому операции возвращаются списком,
                разбитым на страницы. Размером страницы можно управлять через
                поле pager.
                Если данные не найдены, то size в ответе равен 0.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction searching by external transaction ID.
                As a result more than one transaction may be found.
                Transaction list is paged.
                Response element "size" is 0 if no data found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" name="pager" type="tns:Pager">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Настройки страницы данных.
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Long list retrieval settings.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="accountId" type="tns:AccountId">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер счета в системе МОНЕТА.РУ</xsd:documentation>
                        <xsd:documentation xml:lang="en">Account number in MONETA.RU</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="clientTransaction">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Внешний номер операции</xsd:documentation>
                        <xsd:documentation xml:lang="en">External transaction ID</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="tns:CTID">
                            <xsd:minLength value="1"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindOperationsListByCTIDResponse" type="tns:OperationInfoList">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос FindOperationsListByCTIDRequest.
                В результате возвращается список операций, разбитый по страницам.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction searching by external transaction ID response.
                Result contains paged transaction list.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:element name="FindOperationsListRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение списка операций по заданному фильтру.
                Обязательными в фильтре являются только поля с датами периода.
                Все остальные поля в фильтре необязательные.
                Если данные не найдены, то size в ответе равен 0.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction searching by given filter.
                Filter requires date period values to be set.
                Other parameters are optional.
                Response element "size" is 0 if no data found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" name="pager" type="tns:Pager">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Настройки страницы данных. Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Paging settings. Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="filter">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Фильтр, по которому ищем операции.</xsd:documentation>
                        <xsd:documentation xml:lang="en">Transactions filter.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element minOccurs="0" name="unitId" type="xsd:long">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">ID пользователя в системе МОНЕТА.РУ</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Structure identificator in MONETA.RU</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="accountId" type="tns:AccountId">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Номер счета</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Account number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="1" name="dateFrom" type="xsd:dateTime">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Дата начала периода</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Period's start</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="1" name="dateTo" type="xsd:dateTime">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Дата конца периода</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Period's end</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="operationId" type="xsd:long">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Номер операции в системе МОНЕТА.РУ
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">Transaction ID in MONETA.RU
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="amountFrom" type="tns:Money">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Сумма от... Ищутся все операции, которые проходили
                                        на большую или равную сумму. Если указано это поле, то обязательно надо
                                        заполнить Currency
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">Minimal transaction amount.
                                        If specified currencyCode filter parameter is required.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="amountTo" type="tns:Money">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Сумма до... Ищутся все операции, которые проходили
                                        на меньшую или равную сумму. Если указано это поле, то обязательно надо
                                        заполнить Currency
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">Maximal transaction amount.
                                        If specified currencyCode filter parameter is required.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="currencyCode" type="tns:Currency">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Валюта, по которой проходили операции
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">Currency of transaction account.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="targetAccountId" type="tns:AccountId">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Корреспонденский номер счета в системе МОНЕТА.РУ
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">Correspondent account number in MONETA.RU
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="operationStatus" type="tns:OperationStatus">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Статус операции</xsd:documentation>
                                    <xsd:documentation xml:lang="en">Transaction status</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="clientTransaction" type="tns:CTID">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">Внешний номер операции</xsd:documentation>
                                    <xsd:documentation xml:lang="en">External transaction ID</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="operationAmountTypeId" type="xsd:int">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        1 - все операции;
                                        2 - операции, где деньги были получены (сумма &gt; 0);
                                        3 - операции, где деньги были потрачены (сумма &lt; 0).
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        1 - all transactions;
                                        2 - credit transactions;
                                        3 - debit transactions.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="propertyName" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Название свойства операции. Может принимать следующие значения:
                                        YANDEXACCOUNT - счет в системе Яндекс.Деньги;
                                        WEBMONEYWMID - WebMoney WMID;
                                        WEBMONEYPURSE - WebMoney кошелек;
                                        CONTACTTRANSFERORDERNUMBER - Номер перевода в системе Contact;
                                        WIRETRANSFERORDERNUMBER - Номер платежного поручения в банковском переводе;
                                        Значение свойства указывается в поле propertyValue.                                        
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        The transaction property name may take following values:
                                        YANDEXACCOUNT - user's account number in Yandex.Money;
                                        WEBMONEYWMID - user's WMID in WebMoney;
                                        WEBMONEYPURSE - user's purse number in WebMoney;
                                        CONTACTTRANSFERORDERNUMBER - transfer idenitification number in Contact;
                                        WIRETRANSFERORDERNUMBER - bank transfer identification number.
                                        The value of property has to be specified in "propertyValue" element.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="propertyValue" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        Значение свойства операции.
                                        Поиск происходит по прямому совпадению. Для задания маски
                                        можно указать спец-символы - "*" или "?".
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        Transaction property value.
                                        Wildcards "*" and "?" may be used.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="operationCategoryId" type="xsd:long">
                                <xsd:annotation>
                                    <xsd:documentation xml:lang="ru">
                                        1 - Ввод средств;
                                        2 - Вывод средств;
                                        3 - Внутренний перевод;
                                        4 - Товары и услуги.
                                    </xsd:documentation>
                                    <xsd:documentation xml:lang="en">
                                        1 - Deposit;
                                        2 - Withdrawal;
                                        3 - Transfer;
                                        4 - Goods and services.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindOperationsListResponse" type="tns:OperationInfoList">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос FindOperationsListRequest.
                В результате возвращается список операций, разбитый по страницам.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction searching response.
                Result contains paged transactions list.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:element name="FindLastOperationsListRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение списка последних операций.
                Если данные не найдены, то size в ответе равен 0.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Last transactions searching.
                Response element "size" is 0 if no data found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" name="unitId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            ID пользователя в системе МОНЕТА.РУ.
                            Если это поле не задано, то используется текущий пользователь.
                            Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Structure identificator in MONETA.RU.
                            If omitted authenticated user's structure is used.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element default="5" minOccurs="0" name="transactionsQuantity">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Количество операций. Необязательное поле.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Transactions quantity. Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="1"/>
                            <xsd:maxInclusive value="100"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindLastOperationsListResponse" type="tns:OperationInfoList">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос FindLastOperationsListRequest.
                В результате возвращается список последних операций.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Transaction searching response.
                Result contains last transactions list.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:element name="FindProfileInfoByAccountIdRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение данных пользователя по номеру счета.
                Если данные не найдены, возникает Exception.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Request profile information by account number.
                Exception is thrown if no data found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:simpleContent>
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Номер счета пользователя в системе МОНЕТА.РУ</xsd:documentation>
                    <xsd:documentation xml:lang="en">Account number in MONETA.RU</xsd:documentation>
                </xsd:annotation>
                <xsd:extension base="tns:AccountId">
                    <xsd:attribute name="version" type="tns:Version" use="optional"/>
                </xsd:extension>
            </xsd:simpleContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindProfileInfoByAccountIdResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос FindProfileInfoByAccountIdRequest.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Profile information by account number searching response.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="accountId" type="tns:AccountId">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер счета пользователя в системе МОНЕТА.РУ
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Account number in MONETA.RU
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="currency" type="tns:Currency">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Валюта счета</xsd:documentation>
                        <xsd:documentation xml:lang="en">Currency of account</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="profile" type="tns:Profile">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Данные пользователя в системе МОНЕТА.РУ</xsd:documentation>
                        <xsd:documentation xml:lang="en">User profile information in MONETA.RU</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="Profile">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                <p>
                Данные пользователя в системе МОНЕТА.РУ.
                Данные представляются в виде "ключ-значение" и признака подтвержденности.
                Можно получить доступ либо к собственным, либо к публично доступным данным 
                (то есть к тем данным, которые пользователь сам разрешил для просмотра).
                </p>
                <p>
                Возможные ключи для пользователей с типом "client":
                <ul>
                <li>unitid - ID пользователя в системе МОНЕТА.РУ;</li>
                <li>last_name - фамилия;</li>
                <li>first_name - имя;</li>
                <li>middle_initial_name - отчество;</li>
                <li>alias - псевдоним;</li>
                <li>country - страна;</li>
                <li>state - область, республика, штат;</li>
                <li>city - город;</li>
                <li>zip - индекс;</li>
                <li>address - адрес;</li>
                <li>email_for_notifications - адрес электронной почты для уведомлений;</li>
                <li>im_for_notifications - jabber account для уведомлений;</li>    
                <li>phone - номер телефона;</li>
                <li>cell_phone - номер сотового телефона;</li>
                <li>url - url сайта;</li>    
                <li>sex - пол. MALE - мужской, FEMALE - женский;</li>
                <li>date_of_birth - дата рождения (формат - yyyy-mm-dd);</li>
                <li>inn - номер ИНН;</li>
                <li>timezone - часовой пояс пользователя;</li>
                <li>ui_language - язык интерфейса пользователя. RU - руский, EN - английский;</li>
                <li>notification_flags:operation - рассылка уведомлений о проведении операций (0-нет, 1-да);</li>
                <li>notification_flags:login - рассылка уведомлений о входе в систему  (0-нет, 1-да);</li>
                <li>notification_flags:news - подписка на рассылку уведомлений (0-нет, 1-да);</li>
                <li>notification_flags:regular_payments - подписка на рассылку уведомлений по регулярным платежам (0-нет, 1-да);</li>
                <li>notification_flags:balance_threshold - рассылка уведомлений о пороговых значениях баланса (0-нет, 1-да);</li>
                <li>notification_flags_im:operation - рассылка уведомлений о проведении операций (0-нет, 1-да);</li>
                <li>notification_flags_im:login - рассылка уведомлений о входе в систему  (0-нет, 1-да);</li>
                <li>notification_flags_im:news - подписка на рассылку уведомлений (0-нет, 1-да);</li>
                <li>notification_flags_im:regular_payments - подписка на рассылку уведомлений по регулярным платежам (0-нет, 1-да);</li>
                <li>notification_flags_im:balance_threshold - рассылка уведомлений о пороговых значениях баланса (0-нет, 1-да);</li>    
                <li>customfield:* - произвольный набор значений.
                    В данных пользователя их может быть несколько.
                    Полный ключ атрибута состоит из префикса ("customfield:") и тэга (32 символа).
                    Например, "customfield:name".</li>
                </ul>
                </p>

                <p>
                Возможные ключи для пользователей с типом "organization":
                <ul>
                <li>unitid - ID пользователя в системе МОНЕТА.РУ;</li>
                <li>organization_name - название организации;</li>
                <li>alias - псевдоним;</li>
                <li>fio_director - ФИО директора;</li>
                <li>phone_director - телефон директора;</li>
                <li>fio_accountant - ФИО бухгалтера;</li>
                <li>phone_accountant - телефон бухгалтера;</li>
                <li>legal_address - юридический адрес;</li>
                <li>post_address - почтовый адрес;</li>
                <li>actual_address - фактический адрес;</li>
                <li>url - url сайта;</li>
                <li>contact_info - контактная информация;</li>
                <li>contact_email - контактный e-mail;</li>
                <li>business_activity - вид деятельности;</li>
                <li>inn - номер ИНН;</li>
                <li>contract_number - номер договора (только для чтения);</li>
                <li>contract_date - дата договора (только для чтения);</li>    
                <li>timezone - часовой пояс пользователя;</li>
                <li>ui_language - язык интерфейса пользователя. RU - руский, EN - английский;</li>
                <li>notification_flags:operation - рассылка уведомлений о проведении операций (0-нет, 1-да);</li>
                <li>notification_flags:login - рассылка уведомлений о входе в систему  (0-нет, 1-да);</li>
                <li>notification_flags:news - подписка на рассылку уведомлений (0-нет, 1-да);</li>
                <li>notification_flags:regular_payments - подписка на рассылку уведомлений по регулярным платежам (0-нет, 1-да);</li>
                <li>notification_flags:balance_threshold - рассылка уведомлений о пороговых значениях баланса (0-нет, 1-да);</li>    
                <li>notification_flags_im:operation - рассылка уведомлений о проведении операций (0-нет, 1-да);</li>
                <li>notification_flags_im:login - рассылка уведомлений о входе в систему  (0-нет, 1-да);</li>
                <li>notification_flags_im:news - подписка на рассылку уведомлений (0-нет, 1-да);</li>
                <li>notification_flags_im:regular_payments - подписка на рассылку уведомлений по регулярным платежам (0-нет, 1-да);</li>
                <li>notification_flags_im:balance_threshold - рассылка уведомлений о пороговых значениях баланса (0-нет, 1-да);</li>    
                <li>customfield:* - произвольный набор значений.
                    В данных пользователя их может быть несколько.
                    Полный ключ атрибута состоит из префикса ("customfield:") и тэга (32 символа).
                    Например, "customfield:name".</li>
                </ul>
                </p>
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                <p>
                User profile information.
                Data is presented as key-value pairs list.
                Returned attributes are either owned by authenticated user
                or marked as publicly available by profile owner.
                </p>
                <p>
                Possilble keys for "client" profiles:
                <ul>
                <li>unitid - structure identificator in MONETA.RU;</li>
                <li>last_name;</li>
                <li>first_name;</li>
                <li>middle_initial_name;</li>
                <li>alias;</li>
                <li>country;</li>
                <li>state;</li>
                <li>city;</li>
                <li>zip;</li>
                <li>address;</li>
                <li>email_for_notifications;</li>
                <li>im_for_notifications;</li>                                        
                <li>phone;</li>
                <li>cell_phone;</li>
                <li>url;</li>    
                <li>sex (MALE|FEMALE);</li>
                <li>date_of_birth (yyyy-mm-dd format);</li>
                <li>inn - Tax ID;</li>
                <li>timezone;</li>
                <li>ui_language (RU|EN);</li>
                <li>notification_flags:operation - transaction notifications (0-don't send, 1-send);</li>
                <li>notification_flags:login - loging in notifications (0-don't send, 1-send);</li>
                <li>notification_flags:news - news and information notifications (0-don't send, 1-send);</li>
                <li>notification_flags:regular_payments - regular payments notifications (0-don't send, 1-send);</li>
                <li>notification_flags:balance_threshold - account balance threshold notifications (0-don't send, 1-send);</li>    
                <li>notification_flags_im:operation - transaction notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:login - loging in notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:news - news and information notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:regular_payments - regular payments notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:balance_threshold - account balance threshold notifications (0-don't send, 1-send);</li>    
                <li>customfield:* - custom list of values.
                    User profile may contain several attributes with different keys.
                    The full attribute's key consists of prefix ("customfield:") and tag (32 characters).
                    For example, "customfield:name".</li>
                </ul>
                </p>
                <p>
                Possible keys for "organization" profile:
                <ul>
                <li>unitid - structure identificator in MONETA.RU;</li>
                <li>organization_name;</li>
                <li>alias;</li>
                <li>fio_director - Director's Fullname ;</li>
                <li>phone_director - Director's phone;</li>
                <li>fio_accountant - Accountant's fullname;</li>
                <li>phone_accountant - Accountant's phone;</li>
                <li>legal_address - official organization address;</li>
                <li>post_address - post address;</li>
                <li>actual_address - actual address;</li>
                <li>url - site url;</li>
                <li>contact_info - contact info;</li>
                <li>contact_email - contact e-mail;</li>
                <li>business_activity - business activity;</li>
                <li>inn - Tax ID;</li>
                <li>contract_number - contract number;</li>
                <li>contract_date - contract date;</li>    
                <li>timezone - user's time zone;</li>
                <li>ui_language (RU|EN);</li>
                <li>notification_flags:operation - transaction notifications (0-don't send, 1-send);</li>
                <li>notification_flags:login - loging in notifications (0-don't send, 1-send);</li>
                <li>notification_flags:news - news and information notifications (0-don't send, 1-send);</li>
                <li>notification_flags:regular_payments - regular payments notifications (0-don't send, 1-send);</li>
                <li>notification_flags:balance_threshold - account balance threshold notifications (0-don't send, 1-send);</li>    
                <li>notification_flags_im:operation - transaction notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:login - loging in notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:news - news and information notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:regular_payments - regular payments notifications (0-don't send, 1-send);</li>
                <li>notification_flags_im:balance_threshold - account balance threshold notifications (0-don't send, 1-send);</li>    
                <li>customfield:* - custom list of values.
                    Organization profile may contain several attributes with different keys.
                    The full attribute's key consists of prefix ("customfield:") and tag (32 characters).
                    For example, "customfield:name".</li>
                </ul>
                </p>
            </xsd:documentation>
            <xsd:documentation xml:lang="ru">
                Список идентификаторов временных зон, которые используются в системе, доступен на странице <a href="http://en.wikipedia.org/wiki/List_of_zoneinfo_timezones" target="_blank">wikipedia.org</a>
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Timezone identificators supported in MONETA.RU available as TZ name on <a href="http://en.wikipedia.org/wiki/List_of_zoneinfo_timezones" target="_blank">wikipedia.org</a>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="attribute" type="tns:KeyValueApprovedAttribute"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="ProfileType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Тип данных пользователя.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                User profile information type.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="organization">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Тип данных "Организация". В данных такого типа присутствуют поля,
                        характерные для организаций, например: "Название организации", "ФИО директора" и т.п.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        "Organization" profile type containing organization related information.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="client">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Тип данных "Клиент". В данных такого типа присутствуют поля,
                        характерные для обычных пользователей, например: "фамилия", "имя", "отчество" и т.д.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        "Client" profile type containing person related information.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:element name="CreateProfileRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на создание пользователя в системе МОНЕТА.РУ.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                New user registration request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" name="unitId" nillable="true" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            ID родительского пользователя в системе МОНЕТА.РУ, к которому будет принадлежать
                            создаваемый пользователь. Необязательное поле. Если поле не задано,
                            то пользователь будет принадлежать тому пользователю, от имени
                            которого работает web service.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Structure ID in MONETA.RU.
                            If ommited then authenticated user's structure ID is used.
                            Optional.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="profileType" type="tns:ProfileType">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Тип пользователя - "client" или "organization"
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Profile type. See also ProfileType description.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="profile" type="tns:Profile">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Данные создаваемого пользователя. Данные представлены в виде "ключ-значение".
                            Ключи данных зависят от типа пользователя (ProfileType)
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            User profile information in key-value pairs list.
                            Supported keys depend on ProfileType.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="CreateProfileResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос CreateProfileRequest, содержащий ID нового пользователя.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Profile creation response containing registered user's ID.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
            <xsd:restriction base="xsd:long"/>
        </xsd:simpleType>
    </xsd:element>

    <xsd:element name="EditProfileRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на редактирование данных пользователя в системе МОНЕТА.РУ.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Profile information modification request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="unitId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            ID пользователя в системе МОНЕТА.РУ, данные которого надо отредактировать.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Structure ID of profile for modification.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="profile" type="tns:Profile">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Данные редактируемого пользователя. Данные представлены в виде "ключ-значение".
                            Ключи данных зависят от типа редактируемого пользователя (client или organization)
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            User profile information in key-value pairs list.
                            Supported keys depend on ProfileType.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="EditProfileResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который приходит на запрос EditProfileRequest.
                В ответе нет никаких данных.
                Если в процессе сохранения пользователя произошла ошибка, то возникнет Exception.
                Если Exception не возник - значит пользователь гарантированно сохранен.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Profile modification response.
                Response contains no data unless error has occured while saving information
                and Exception returned as a result.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType/>
    </xsd:element>

    <xsd:element name="GetProfileInfoRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение данных пользователя по ID пользователя.
                Если данные не найдены, возникает Exception.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Request for profile information by structure ID.
                Exception is thrown if no data found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:Entity">
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="unitId" type="xsd:long">
                            <xsd:annotation>
                                <xsd:documentation xml:lang="ru">
                                    ID пользователя в системе МОНЕТА.РУ.
                                    Если это поле не задано, то используется текущий пользователь.
                                    Необязательное поле.
                                </xsd:documentation>
                                <xsd:documentation xml:lang="en">
                                    Structure ID.
                                    If omitted authenticated user's structure is used.
                                    Optional.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="GetProfileInfoResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Ответ на запрос GetProfileInfoRequest.</xsd:documentation>
            <xsd:documentation xml:lang="en">Profile information by structure ID searching response.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Данные пользователя в системе МОНЕТА.РУ</xsd:documentation>
                    <xsd:documentation xml:lang="en">User profile information in MONETA.RU</xsd:documentation>
                </xsd:annotation>
                <xsd:extension base="tns:Profile"/>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindProfileInfoRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Поиск пользователя в системе МОНЕТА.РУ по заданному фильтру.
                В результате возвратится список пользователей, разбитый на страницы
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Profile searching by filter parameters.
                Result contains paged profile list.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="tns:Entity">
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="pager" type="tns:Pager">
                            <xsd:annotation>
                                <xsd:documentation xml:lang="ru">Настройки страницы данных. Необязательное поле.
                                </xsd:documentation>
                                <xsd:documentation xml:lang="en">Paging settings. Optional.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="1" name="filter">
                            <xsd:annotation>
                                <xsd:documentation xml:lang="ru">Фильтр, по которому ищем данные</xsd:documentation>
                                <xsd:documentation xml:lang="en">Searching filter</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element minOccurs="0" name="unitId" nillable="true" type="xsd:long">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">
                                                ID пользователя в системе МОНЕТА.РУ. Необязательное поле. ID пользователя "от которого"
                                                начинается поиск. Если это поле не задано, то ищем пользователей, начиная
                                                от идентифицированного пользователя.
                                            </xsd:documentation>
                                            <xsd:documentation xml:lang="en">
                                                Structure ID of root where searching starts from.
                                                If ommited then authenticated user's structure ID is used.
                                                Optional.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="name" type="xsd:string">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">
                                                Имя, по которому происходит поиск. Необязательное поле. Если поле задано,
                                                то оно должно быть не меньше 2-х символов.
                                                Поиск происходит по прямому совпадению. Для задания маски
                                                можно указать спец-символы - "*" или "?".
                                                Для пользователей с типом "organization" поиск происходит по
                                                полю "organization_name". Для пользователей с типом "client" -
                                                по общему полю "last_name first_name middle_initial_name".
                                                То есть, если нужно найти пользователя "Иванов Андрей Сергеевич",
                                                то следует ввести имя для поиска именно так. Если нужно найти всех
                                                пользователей с фамилией "Иванов", то запрос должен быть "Иванов*".
                                                Если нужно найти всех пользователей с именем "Андрей", то запрос
                                                должен быть "*Андрей*".
                                            </xsd:documentation>
                                            <xsd:documentation xml:lang="en">
                                                Name to search for. Optional.
                                                If specified must be not shorter than 2 symbols.
                                                Wildcards "*" and "?" may be used.
                                                For "organization" profiles "organization_name" is matched.
                                                For "client" profiles "last_name first_name middle_initial_name" is matched.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="email" type="xsd:string">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">
                                                Email, по которому происходит поиск. Необязательное поле. Если поле задано,
                                                то оно должно быть не меньше 2-х символов.
                                                Поиск происходит по прямому совпадению. Для задания маски
                                                можно указать спец-символы - "*" или "?".
                                            </xsd:documentation>
                                            <xsd:documentation xml:lang="en">
                                                Email to search for. Optional.
                                                If specified must be not shorted than 2 symbols.
                                                Wildcards "*" and "?" may be used.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="accountBalanceFrom" type="tns:Money">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">
                                                Возвращаются пользователи у которых есть счета с балансом
                                                большим или равным указанному значению.
                                                Необязательное поле.
                                            </xsd:documentation>
                                            <xsd:documentation xml:lang="en">
                                                Only profiles with greater or equal account's balance are returned.
                                                Optional.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="accountBalanceTo" type="tns:Money">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">
                                                Возвращаются пользователи у которых есть счета с балансом
                                                меньшим или равным указанному значению.
                                                Необязательное поле.
                                            </xsd:documentation>
                                            <xsd:documentation xml:lang="en">
                                                Only profiles with less or equal account's balance are returned.
                                                Optional.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="accountCurrencyCode" type="tns:Currency">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">
                                                При указании границ баланса в поиске можно уточнить валюту счета.
                                                Необязательное поле.
                                            </xsd:documentation>
                                            <xsd:documentation xml:lang="en">
                                                If balance limits are specified only accounts with given currency will be analysed.
                                                Optional.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindProfileInfoResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, который приходит на запрос FindProfileInfoRequest.
                Данные в ответе разбиты по страницам.
                Если данные не найдены, то size в ответе равен 0.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Profile searching response.
                Result is paged.
                Response element "size" is 0 if no data found.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="pageSize" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Количество пользователей, возвращаемых в результате запроса.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Profiles per page.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="pageNumber" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Номер текущей страницы. Нумерация начинается с 1
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Current page number starting from 1.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="pagesCount" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Сколько всего страниц с пользователями возможно по данному
                            запросу
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Total count of pages.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="size" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Размер текущей страницы. Он всегда &lt;= pageSize. На последней
                            странице может быть &lt; pageSize
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Profiles count in current page.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="totalSize" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Общее количество пользователей, которое можно получить в данной
                            выборке
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">Total profiles count matched by searching filter.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="profile" type="tns:Profile">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">Список пользователей</xsd:documentation>
                        <xsd:documentation xml:lang="en">Profile list</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:simpleType name="DocumentType">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Типы документов, поддерживаемые в системе МОНЕТА.РУ.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Document type supported in MONETA.RU.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="PASSPORT">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Паспорт</xsd:documentation>
                    <xsd:documentation xml:lang="en">Civil passport</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="DRIVING_LICENCE">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Водительское удостоверение</xsd:documentation>
                    <xsd:documentation xml:lang="en">User's driving licence</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="MILITARY_ID">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Военный билет</xsd:documentation>
                    <xsd:documentation xml:lang="en">Military ID</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="OTHER">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Другой документ</xsd:documentation>
                    <xsd:documentation xml:lang="en">Other document</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="Document">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Тип, описывающий документ в системе МОНЕТА.РУ</xsd:documentation>
            <xsd:documentation xml:lang="en">Document information type</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="id" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Идентификатор документа в системе МОНЕТА.РУ</xsd:documentation>
                    <xsd:documentation xml:lang="en">Document identificator in MONETA.RU</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="type" type="tns:DocumentType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Тип документа</xsd:documentation>
                    <xsd:documentation xml:lang="en">Document type</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="attribute" type="tns:KeyValueApprovedAttribute">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Поля документа в системе МОНЕТА.РУ.
                        Данные представляются в виде "ключ-значение" и признака подтвержденности.
                        В зависимости от типа документа возвращаются следующие поля:
                        Для документов типа PASSPORT, MILITARY_ID:
                        <ul>
                            <li>SERIES - серия документа</li>
                            <li>NUMBER - номер документа</li>
                            <li>ISSUER - кем выдан документ</li>
                            <li>ISSUED - когда выдан документ</li>
                            <li>COMMENTS - комментарии (необязательное поле)</li>
                        </ul>
                        Для документов типа DRIVING_LICENCE:
                        <ul>
                            <li>SERIES - серия документа</li>
                            <li>NUMBER - номер документа</li>
                            <li>ISSUER - кем выдан документ</li>
                            <li>ISSUED - когда выдан документ</li>
                            <li>EXPIRATIONDATE - срок действия</li>
                            <li>COMMENTS - комментарии (необязательное поле)</li>
                        </ul>
                        Для OTHER:
                        <ul>
                            <li>COMMENTS - комментарии, пояснения, описание</li>
                        </ul>
                        Для всех типов документов:
                        <ul>
                            <li>customfield:* - произвольный набор значений.
                                В документе их может быть несколько.
                                Полный ключ атрибута состоит из префикса ("customfield:") и тэга (32 символа).
                                Например, "customfield:name".</li>
                            <li>MODIFICATIONDATE - последняя дата редактирования документа</li>
                        </ul>
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        Document attributes in MONETA.RU.
                        Information is represented by key-value pairs list.

                        Supported keys depending on document type as follows.
                        PASSPORT, MILITARY_ID document type:
                        <ul>
                            <li>SERIES - document series</li>
                            <li>NUMBER - document number</li>
                            <li>ISSUER - document issuer</li>
                            <li>ISSUED - document issuance date</li>
                            <li>COMMENTS - optional description</li>
                        </ul>
                        DRIVING_LICENCE document type:
                        <ul>
                            <li>SERIES - document series</li>
                            <li>NUMBER - document number</li>
                            <li>ISSUER - document issuer</li>
                            <li>ISSUED - document issuance date</li>
                            <li>EXPIRATIONDATE - expiration date</li>
                            <li>COMMENTS - optional description</li>
                        </ul>
                        OTHER document type:
                        <ul>
                            <li>COMMENTS - name, description, comments or explanation</li>
                        </ul>
                        all document types:
                        <ul>
                            <li>customfield:* - custom list of values.
                                The document may contain several attributes with different keys.
                                The full attribute's key consists of prefix ("customfield:") and tag (32 characters).
                                For example, "customfield:name".</li>
                            <li>MODIFICATIONDATE - document last modification date</li>
                        </ul>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="hasAttachedFiles" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">
                        Имеет ли документ прикрепленные файлы.
                        Для получения прикрепленных файлов используйте вызов FindProfileDocumentFilesRequest 
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">
                        If the document has attachments or not.
                        For attachment fetching see FindProfileDocumentFilesRequest
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="File">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Тип, описывающий данные в виде бинарного файла.</xsd:documentation>
            <xsd:documentation xml:lang="en">Binary data type.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentId" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">ID документа, которому принадлежит данный файл</xsd:documentation>
                    <xsd:documentation xml:lang="en">Document ID, which file belongs to</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="blob" type="xsd:base64Binary">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Данные файла. При передаче/получении данных используйте MTOM (Message Transmission Optimization Mechanism).</xsd:documentation>
                    <xsd:documentation xml:lang="en">File contents. Use MTOM (Message Transmission Optimization Mechanism).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="approved" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Проверен или нет данный файл</xsd:documentation>
                    <xsd:documentation xml:lang="en">Is file approved or not</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="fileId" type="xsd:long">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">ID файла</xsd:documentation>
                    <xsd:documentation xml:lang="en">File Id</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="mimeType" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Mime type файла (например: image/jpeg)</xsd:documentation>
                    <xsd:documentation xml:lang="en">File mime type (example: image/jpeg)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="title" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Имя файла или описание</xsd:documentation>
                    <xsd:documentation xml:lang="en">File name or description</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="FindProfileDocumentFilesRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение бинарных данных, ассоциированных с указанным документом.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Request for binary data associated with given document.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="documentId" type="xsd:long"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="FindProfileDocumentFilesResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ, содержащий бинарные данные, ассоциированные с указанным в запросе документом.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Response containing binary data associated with document given in request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="file" type="tns:File"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="UploadProfileDocumentFileRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Запрос на создание/редактирование файла документа</xsd:documentation>
            <xsd:documentation xml:lang="en">Request for file adding/modification.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="file" type="tns:File">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            Если указано поле fileId - файл редактируется, иначе создается новый файл.
                            Поле approved в данном запросе не используется.
                            Если не указан mimeType, но в запросе есть имя файла (поле title), то mimeType будет вычисляться по расширению файла
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Specify "fileId" to modify file, omit "fileId" to add new file.
                            Approved is not used.
                            If "mimeType" omitted and "title" is specified, "mimeType" will be resolved by filename extension.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="UploadProfileDocumentFileResponse">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Ответ на запрос создания/редактирования файла документа
                Если в процессе сохранения файла произошла ошибка, то возникнет Exception.
                Если Exception не возник - значит файл гарантированно сохранен.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Response for file adding/modification.
                Response contains no data unless some error has occured while modification.
                If reponse has no Exception then file is uploaded successfully.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType/>
    </xsd:element>

    <xsd:element name="FindProfileDocumentsRequest">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">
                Запрос на получение документов пользователя.
            </xsd:documentation>
            <xsd:documentation xml:lang="en">
                Profile's documents retrieval request.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="unitId" type="xsd:long">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="ru">
                            ID пользователя, документы которого запрашиваются.
                        </xsd:documentation>
                        <xsd:documentation xml:lang="en">
                            Structure ID containing documents.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
